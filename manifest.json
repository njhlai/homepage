{
    "manifest_version": 2,
    "name": "minimal-dark-homepage",
    "short_name": "homepage",
    "version": "2.1.0",
    "description": "Overrides the browser's newtab page with a custom homepage.",
    "homepage_url": "https://github.com/njhlai/homepage",
    "author": "njhlai",
    "icons": {
        "528": "img/icon.png"
    },
    "options_ui": {
        "page": "option.html",
        "browser_style": true
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "chrome_settings_overrides": {
        "homepage": "index.html"
    }
}
